public class task540 {
    public static void main(String[] args) {
        int[] nums = {3,3,7,7,10,11,11};
        System.out.println(singleNonDuplicate(nums));
    }
      public static int  singleNonDuplicate(int[] nums) {
        int element =0;
        for (int i:nums ) {
          element ^=i;
        }
        return element;
      }
      }
